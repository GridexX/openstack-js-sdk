{
  "name": "openstack-js-sdk",
  "version": "1.0.0",
  "description": "A simple OpenStack SDK for JavaScript and TypeScript",
  "main": "out/lib/converter.js",
  "bin": {
    "openstack-js-sdk": "out/index.js"
  },
  "files": [
    "out/"
  ],
  "scripts": {
    "test": "hurl --test tests/**",
    "dev:start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.[tj]s\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GridexX/openstack-js-sdk"
  },
  "keywords": [
    "openstack",
    "sdk",
    "javascript",
    "typescript",
    "cloud"
  ],
  "author": {
    "name": "Ars√®ne Fougerouse",
    "email": "arsene582@gmail.com",
    "url": "https://github.com/GridexX"
  },
  "homepage": "https://github.com/GridexX",
  "license": "MIT",
  "devDependencies": {
    "@orangeopensource/hurl": "^4.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^3.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/pino": "^7.0.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.1.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "js-yaml": "^4.1.0",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  }
}
